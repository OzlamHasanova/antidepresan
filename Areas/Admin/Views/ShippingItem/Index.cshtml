@model IEnumerable<ShippingItem>
@{
    ViewData["Title"] = "Index";
    
}

<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Striped Table</h4>
            <p class="card-description">
                Add class <code>.table-striped</code>
            </p>
            <div class="table-responsive">
                <table class="table table-striped">
                    <thead>
                        <tr>
                            <th>
                                Image
                            </th>
                            <th>
                                Title
                            </th>
                            <th>
                                Descrption
                            </th>
                            <th>
                                Settings
                            </th>
                            
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var shipping in Model)
                        {
                            <tr>
                                <td class="py-1">
                                    <img src="~/assets/images/shipping/icon/@shipping.Image" alt="image" />
                                </td>
                                <td>
                                    @shipping.Title
                                </td>
                                <td>
                                    <a href="/detail/shipping.Id" class="btn btn-primary">
                                        Detail
                                        <i class="mdi mdi-information"></i>
                                    </a>

                                    <a href="/update/shipping.Id" class="btn btn-warning">
                                        Update
                                        <i class="mdi mdi-pencil-box"></i>
                                    </a>
                                    <a href="/delete/shipping.Id" class="btn btn-danger">
                                        Delete
                                        <i class="mdi mdi-delete-sweep"></i>
                                    </a>
                                </td>
                                

                            </tr>

                        }
                        
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

